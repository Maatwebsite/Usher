{
  "name": "maatwebsite/usher",
  "description": "A Doctrine ACL package for Laravel",
  "license": "MIT",
  "keywords": [
    "laravel",
    "doctrine",
    "orm",
    "acl",
    "users",
    "roles"
  ],
  "authors": [
    {
      "name": "Maatwebsite.nl",
      "email": "patrick@maatwebsite.nl"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "brouwers/laravel-doctrine": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.5",
    "phpspec/phpspec": "~2.1@dev",
    "mockery/mockery": "0.9.*@dev"
  },
  "autoload": {
    "psr-4": {
      "Maatwebsite\\Usher\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Maatwebsite\\Usher\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
