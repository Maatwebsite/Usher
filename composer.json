{
  "name": "maatwebsite/usher",
  "description": "A Doctrine ACL package for Laravel",
  "license": "MIT",
  "keywords": [
    "laravel",
    "doctrine",
    "orm",
    "acl",
    "users",
    "roles"
  ],
  "authors": [
    {
      "name": "Maatwebsite.nl",
      "email": "patrick@maatwebsite.nl"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:Maatwebsite/laravel-doctrine.git"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "beberlei/assert": "~2.3",
    "mitchellvanw/laravel-doctrine": "dev-develop"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.5",
    "phpspec/phpspec": "~2.1@dev"
  },
  "autoload": {
    "psr-4": {
      "Maatwebsite\\Usher\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Maatwebsite\\Usher\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
